func foo() {
    print "In foo()";
    bar(1, 2);
}

func bar(a, b) {
    print "In bar(a, b)";
    print a;
    print b;
}

foo();
bar(3, 4);

func factorial(target, curProduct) {
    if (target == 0)
        print curProduct;
    else
        factorial(target - 1, curProduct * target);
}

print "Factorials:";
factorial(1, 1);
factorial(2, 1);
factorial(3, 1);
factorial(4, 1);

print factorial;